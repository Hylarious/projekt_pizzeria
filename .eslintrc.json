{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 2015,
    "sourceType": "module"
  },
  "rules": {
    "linebreak-style": ["off"],
    "semi": ["error", "always"],
    "no-console": ["off"]
  },
  "globals": {
    "Handlebars": false
  }
}
